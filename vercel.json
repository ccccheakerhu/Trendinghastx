{
  "version": 2,
  "functions": {
    "api/trending.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/trending"
    },
    {
      "src": "/trending",
      "dest": "/api/trending"
    },
    {
      "src": "/trending/(?<country>[^/]+)",
      "dest": "/api/trending?country=$country"
    },
    {
      "src": "/api/trending/(?<country>[^/]+)",
      "dest": "/api/trending?country=$country"
    }
  ]
}
